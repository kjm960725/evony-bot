# Discord Bot Token
# Discord Developer Portal (https://discord.com/developers/applications)에서 발급받은 토큰을 입력하세요
DISCORD_TOKEN=your_discord_bot_token_here

# iScout Evony Website Credentials
ISCOUT_URL=https://www.iscout.club/en
ISCOUT_EMAIL=your_email@example.com
ISCOUT_PASSWORD=your_password_here

# Prisma Database
DATABASE_URL="file:./dev.db"

# Puppeteer Chrome 경로 (선택사항, 자동 감지됨)
# macOS: /Applications/Google Chrome.app/Contents/MacOS/Google Chrome
# Linux: /usr/bin/chromium 또는 /usr/bin/chromium-browser
# PUPPETEER_EXECUTABLE_PATH=/Applications/Google Chrome.app/Contents/MacOS/Google Chrome

# Puppeteer Headless 모드 (개발: false, 서버: true)
# false: 브라우저 창 표시 (디버깅 가능)
# true: headless 모드 (백그라운드 실행)
PUPPETEER_HEADLESS=false
